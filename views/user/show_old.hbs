<div id="order-container">
  <h2>Детали заказа</h2>
  <p>Ресторан: {{this.title}}</p>
  <p>Состав заказа:
  <ul>
    {{#each positions}}
    <li>{{this.product}} - x{{this.quantity}}</li>
    {{/each}}
  </ul>
  </p>
  <p>Цена: {{this.price}}</p>

  <p>Адрес: {{this.address}}</p>
  <p>Дата создания: {{this.date}}</p>
  <p>Комментарий: {{this.comment}}</p>
</div>

<a href="/user/main">На главную</a>
